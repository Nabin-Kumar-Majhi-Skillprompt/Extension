{
    "manifest_version": 3,
    "name": "Login Extension",
    "version": "1.0",
    "description": "Simple login chrome extension",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
      }
    },
    "permissions": [
      "storage",
      "activeTab"
    ]
  }